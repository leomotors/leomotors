{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "format": {
      "cache": false
    },
    "load": {
      "dependsOn": [
        "build",
        "^build"
      ],
      "outputs": [
        "data/data.json"
      ]
    },
    "analyze": {
      "dependsOn": [
        "load",
        "^load"
      ],
      "outputs": [
        "data/lang.csv"
      ]
    }
  }
}