{
  "name": "@leomotors/language-stats",
  "version": "2.0.2",
  "description": "Get Summary of Your Language Stats",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "load": "mkdir -p data && node dist/src/loadData.js",
    "analyze": "node dist/src/analyze.js",
    "gql-codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write .*.cjs src/**/*.{js,ts}"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.4",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@leomotors/config": "workspace:0.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "graphql": "^16.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6"
  }
}