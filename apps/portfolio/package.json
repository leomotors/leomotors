{
  "name": "portfolio.leomotors.net",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm lm-config && vite dev --port=5274",
    "build": "pnpm lm-config && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src *.{cjs,js} --max-warnings=0",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" \"src/**/*.{ts,svelte,css,html,scss}\""
  },
  "devDependencies": {
    "@leomotors/config": "workspace:0.0.0",
    "@leomotors/data": "workspace:0.0.0",
    "@leomotors/scripts": "^3.1.0",
    "@leomotors/sv-components": "^0.4.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.72",
    "@sveltejs/kit": "1.0.0-next.472",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "autoprefixer": "^10.4.8",
    "chalk": "^5.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.8",
    "svelte": "^3.50.0",
    "svelte-bootstrap-icons": "^2.2.0",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0"
  }
}